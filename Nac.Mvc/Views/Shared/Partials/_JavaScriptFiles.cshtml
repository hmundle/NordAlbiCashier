@using Nac.Lib.Utilities
@using Nac.Mvc.Controllers

<!-- Partials/_JavaScriptFiles -->
<environment include="Development">
    <script src="~/lib/jquery/jquery.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/js/bootstrap.bundle.js" asp-append-version="true"></script>
</environment>
<environment exclude="Development">
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
</environment>
@{
    if (ViewData["UseDatatables"] as bool? == true || ViewData["UseDatatablesInsert"] as bool? == true)
    {
        <environment include="Development">
            <script src="~/lib/datatables.net/js/jquery.dataTables.js"></script>
            <script src="~/lib/datatables.net-searchpanes/js/dataTables.searchPanes.js"></script>
            <script src="~/lib/datatables.net-select/js/dataTables.select.js"></script>
            <script src="~/lib/datatables.net-buttons/js/dataTables.buttons.js"></script>
            <script src="~/lib/datatables.net-buttons/js/buttons.html5.js"></script>
            <script src="~/lib/datatables.net-datetime/dist/dataTables.dateTime.js"></script>
            <script src="~/lib/datatables.net-searchbuilder/js/dataTables.searchBuilder.js"></script>
            <script src="~/lib/jszip/dist/jszip.js"></script>

            <script src="~/lib/datatables.net-bs5/js/dataTables.bootstrap5.js"></script>
            <script src="~/lib/datatables.net-searchpanes-bs5/js/searchPanes.bootstrap5.js"></script>
            <script src="~/lib/datatables.net-select-bs5/js/select.bootstrap5.js"></script>
            <script src="~/lib/datatables.net-buttons-bs5/js/buttons.bootstrap5.js"></script>
            <script src="~/lib/datatables.net-searchbuilder-bs5/js/searchBuilder.bootstrap5.js"></script>

        </environment>
        <environment exclude="Development">
            <script src="~/lib/datatables.net/js/jquery.dataTables.min.js"></script>
            <script src="~/lib/datatables.net-searchpanes/js/dataTables.searchPanes.min.js"></script>
            <script src="~/lib/datatables.net-select/js/dataTables.select.min.js"></script>
            <script src="~/lib/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
            <script src="~/lib/datatables.net-buttons/js/buttons.html5.min.js"></script>
            <script src="~/lib/datatables.net-datetime/dist/dataTables.dateTime.min.js"></script>
            <script src="~/lib/datatables.net-searchbuilder/js/dataTables.searchBuilder.min.js"></script>
            <script src="~/lib/jszip/dist/jszip.min.js"></script>

            <script src="~/lib/datatables.net-bs5/js/dataTables.bootstrap5.min.js"></script>
            <script src="~/lib/datatables.net-searchpanes-bs5/js/searchPanes.bootstrap5.min.js"></script>
            <script src="~/lib/datatables.net-select-bs5/js/select.bootstrap5.min.js"></script>
            <script src="~/lib/datatables.net-buttons-bs5/js/buttons.bootstrap5.min.js"></script>
            <script src="~/lib/datatables.net-searchbuilder-bs5/js/searchBuilder.bootstrap5.min.js"></script>
        </environment>
    }
}
<script src="~/js/site.js" asp-append-version="true"></script>
